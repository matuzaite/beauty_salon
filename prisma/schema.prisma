generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model services {
  id    BigInt   @default(autoincrement())
  name  String   @default("250") @db.VarChar
  price Decimal? @db.Decimal

  @@ignore
}

model specialists {
  id             BigInt  @id @default(autoincrement())
  name           String? @default("255") @db.VarChar
  specialization String? @default("255") @db.VarChar
  phone          String? @default("50") @db.VarChar
}
