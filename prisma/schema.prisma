datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model services {
  id        BigInt   @id @default(autoincrement())
  name      String
}

model specialists {
  id             BigInt   @id @default(autoincrement())
  name           String
  specialization String?
  phone          String?
}
